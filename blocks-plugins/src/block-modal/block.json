{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "stitchedblocks/block-modal-content",
	"version": "0.1.0",
	"title": "Modal Content",
	"category": "widgets",
	"icon": "testimonial",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"message": {
			"type": "string",
			"source": "text",
			"selector": "div",
			"default": ""
		},
		"columns": { 
			"type": "number", 
			"default": 3 
		},
		"tabidentifier": {
			"type": "string",
			"default": "Select an Option"
		}
	},
	"textdomain": "modalcontent",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./css/block-modal-back-end.css",
	"style": "file:./css/block-modal-front-end.css",
	"viewScript": "file:./view.js"
}
