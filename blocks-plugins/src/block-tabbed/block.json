{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "stitchedblocks/block-tabbed-content",
	"version": "0.1.0",
	"title": "Tabbed Content Block",
	"category": "widgets",
	"icon": "slides",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"message": {
			"type": "string",
			"source": "text",
			"selector": "div",
			"default": ""
		},
		"columns": { 
			"type": "number", 
			"default": 3 
		},
		"sttb1__tabbedlink": {
			"type": "string",
			"default": "Select an Option"
		},
        "sttb1__tabbedlinksubelement": {
            "type": "string",
			"default": "Select an Option"
        }
	},
	"textdomain": "tabbedcontent",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./css/block-tabbed-back-end.css",
	"style": "file:./css/block-tabbed-front-end.css",
	"viewScript": "file:./view.js"
}
